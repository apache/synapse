<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>Apache-Synapse</display-name>
    <servlet>
        <servlet-name>SynapseServlet</servlet-name>
        <display-name>Apache-Synapse Servlet</display-name>
        <servlet-class>
        org.apache.synapse.SynapseServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SynapseServlet</servlet-name>
        <url-pattern>/servlet/SynapseServlet</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>SynapseServlet</servlet-name>
        <url-pattern>/services/*</url-pattern>
    </servlet-mapping>
</web-app>
